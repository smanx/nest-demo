{
    "version": 2,
    "builds": [
        {
            "src": "yarn",
            "use": "@vercel/node"
        },
        {
            "src": "yarn build",
            "use": "@vercel/node"
        },
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}
